{
  "compilerOptions": {
    "jsx": "react-jsx",
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "verbatimModuleSyntax": true,
    "outDir": "./dist",
    "lib": [
      "ES2023",
      "DOM"
    ],
    "target": "ESNext",
    "module": "NodeNext"
  },
  "include": [
    "./src"
  ]
}
